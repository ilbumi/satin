# Satin Configuration Example
# Copy this file to .env and modify the values as needed

# MongoDB connection string
SATIN_MONGO_DSN="mongodb://localhost:27017/satin"

# Backend server settings
SATIN_BACKEND_HOST="localhost"
SATIN_BACKEND_PORT=8000
SATIN_RELOAD=false

# CORS allowed origins (JSON array format)
SATIN_CORS=["http://localhost:5173"]
