{
	"name": "frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit:server": "vitest --run --project=server",
		"test:unit:client": "node scripts/run-client-tests.js",
		"test:unit": "pnpm run test:unit:server && pnpm run test:unit:client",
		"test:e2e": "playwright test",
		"test": "pnpm run test:unit && pnpm run test:e2e",
		"codegen": "graphql-codegen --config codegen.yml",
		"codegen:watch": "graphql-codegen --config codegen.yml --watch"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.33.0",
		"@graphql-codegen/cli": "^5.0.7",
		"@graphql-codegen/typescript": "^4.1.6",
		"@graphql-codegen/typescript-operations": "^4.6.1",
		"@graphql-codegen/urql-introspection": "^3.0.1",
		"@playwright/test": "^1.54.2",
		"@sveltejs/adapter-node": "^5.3.1",
		"@sveltejs/kit": "^2.33.0",
		"@sveltejs/vite-plugin-svelte": "^6.1.2",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.12",
		"@testing-library/svelte": "^5.2.8",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^24.3.0",
		"@vitest/browser": "^3.2.4",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"eslint-plugin-tailwindcss": "4.0.0-beta.0",
		"globals": "^16.3.0",
		"jsdom": "^26.1.0",
		"playwright": "^1.54.2",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.38.2",
		"svelte-check": "^4.3.1",
		"svelte2tsx": "^0.7.42",
		"tailwindcss": "^4.1.12",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.40.0",
		"vite": "^7.1.3",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^1.1.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"dependencies": {
		"@urql/core": "^6.0.1",
		"@urql/exchange-graphcache": "^8.1.0",
		"@urql/svelte": "^5.0.0",
		"canvas": "^3.2.0",
		"graphql": "^16.11.0",
		"konva": "^9.3.22",
		"wonka": "^6.3.5",
		"zod": "^4.0.17"
	}
}
